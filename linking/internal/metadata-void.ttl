########################################################
#                   LINKSETS
########################################################

:internal-cw-ls-country a void:Linkset ;
    dcterms:title           "Covid World countries linked to Life Satisfaction countries."@en ;
    dcterms:description     "Linkset connecting countries from covid-world dataset to countries from life-satisfacton dataset."@en ;
    void:target             :covid-world ;
    void:target             :life-satisfaction ;
    void:linkPredicate      owl:sameAs ;
    dcterms:creator         <http://www.example.org/contacts/pondepe1> ;
    dcterms:publisher       <http://www.example.org/contacts/pondepe1> ;
    dcterms:contributor     <http://www.example.org/contacts/pondepe1> ;
    dcterms:created         "2021-11-25"^^xsd:date ;
    dcterms:modified        "2021-11-25"^^xsd:date ;
    void:feature            <http://www.w3.org/ns/formats/N-Triples> .

:internal-cw-ccz-observation a void:Linkset ;
    dcterms:title           "Covid World observation linked to Covid CZE observations restricted to Prague."@en ;
    dcterms:description     "Linkset connecting observations from covid-world dataset to observations from covid-cze dataset. Observations from covid-cze were restricted only to Prague due to enormous number of generated links."@en ;
    void:target             :covid-world ;
    void:target             :covid-cze ;
    void:linkPredicate      owl:sameAs ;
    dcterms:creator         <http://www.example.org/contacts/pondepe1> ;
    dcterms:publisher       <http://www.example.org/contacts/pondepe1> ;
    dcterms:contributor     <http://www.example.org/contacts/pondepe1> ;
    dcterms:created         "2021-11-25"^^xsd:date ;
    dcterms:modified        "2021-11-25"^^xsd:date ;
    void:feature            <http://www.w3.org/ns/formats/N-Triples> .

:internal-ccz-okresy-okres a void:Linkset ;
    dcterms:title           "Propojení okresů z Covid CZE a číselníku okresů."@cs ;
    dcterms:description     "Linkset propojující okresy z datasetu covid-cze s okresy z číselníku okresů."@cs ;
    void:target             :covid-cze ;
    void:target             :okresy ;
    void:linkPredicate      owl:sameAs ;
    dcterms:creator         <http://www.example.org/contacts/pondepe1> ;
    dcterms:publisher       <http://www.example.org/contacts/pondepe1> ;
    dcterms:contributor     <http://www.example.org/contacts/pondepe1> ;
    dcterms:created         "2021-11-25"^^xsd:date ;
    dcterms:modified        "2021-11-25"^^xsd:date ;
    void:feature            <http://www.w3.org/ns/formats/N-Triples> .

:internal-ccz-kraje-kraj a void:Linkset ;
    dcterms:title           "Propojení krajů z Covid CZE a číselníku krajů."@cs ;
    dcterms:description     "Linkset propojující kraje z datasetu covid-cze s kraji z číselníku krajů."@cs ;
    void:target             :covid-cze ;
    void:target             :kraje ;
    void:linkPredicate      owl:sameAs ;
    dcterms:creator         <http://www.example.org/contacts/pondepe1> ;
    dcterms:publisher       <http://www.example.org/contacts/pondepe1> ;
    dcterms:contributor     <http://www.example.org/contacts/pondepe1> ;
